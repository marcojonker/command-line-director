{
  "name": "command-line-director",
  "version": "2.0.0-beta.2",
  "description": "Gives direction to you nodejs command line application",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "keywords": [
    "command line",
    "command line parser",
    "command line mapper",
    "command line arguments",
    "arguments"
  ],
  "bugs": "https://github.com/marcojonker/command-line-director/issues",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist",
    "prepare": "npm run build",
    "test": "jest --coverage",
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix"
  },
  "author": "Marco Jonker",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "CHANGELOG",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marcojonker/command-line-director.git"
  },
  "homepage": "http://www.cacadu.eu/opensource",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^20.14.12",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
  ,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
